<%-include ("./component/header")%>
<div class="container">
  <h1>Welcome to Facebook</h1>
  <h2>View post</h2>
  <div class="form-group">
    <label>Title</label>
    <%= result.name %>
  </div>

  <div class="form-group">
    <label>Article</label>
    <%= result.message %>
  </div>
  <p class="card-text"><%= result.createdAt.toLocaleString() %></p>

  <a href="/feed/edit/<%= result.id %>" class="btn btn-primary">EDIT</a>
  <a href="/feed/delete/<%= result.id %>" class="btn btn-primary">DELETE</a>
  <a href="/" class="btn btn-primary">CANCEL</a>
</div>
